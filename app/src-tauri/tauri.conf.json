{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Etcd Workbench",
    "version": "1.2.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      },
      "os": {
        "all": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "Etcd Workbench",
        "width": 1400,
        "height": 1000,
        "center": true,
        "decorations": false,
        "transparent": true,
        "visible": false
      },
      {
        "label": "setting",
        "title": "Etcd Workbench",
        "width": 1200,
        "height": 800,
        "center": true,
        "url": "/?page=setting",
        "decorations": false,
        "transparent": true,
        "visible": false
      },
      {
        "label": "splashscreen",
        "width": 350,
        "height": 100,
        "center": true,
        "url": "splashscreen.html",
        "decorations": false,
        "resizable": false,
        "transparent": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.beifengtz.etcdworkbench",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
