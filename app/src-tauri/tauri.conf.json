{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "Etcd Workbench",
    "version": "0.0.2"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "all": true
      },
      "os": {
        "all": true
      },
      "dialog": {
        "open": true,
        "save": true
      }
    },
    "windows": [
      {
        "label": "main",
        "title": "Etcd Workbench",
        "width": 1200,
        "height": 900,
        "minWidth": 900,
        "minHeight": 500,
        "center": true,
        "decorations": false,
        "transparent": true,
        "visible": false,
        "closable": true,
        "focus": false,
        "titleBarStyle": "Overlay"
      },
      {
        "label": "setting",
        "title": "Settings",
        "width": 1000,
        "height": 750,
        "resizable": false,
        "center": true,
        "url": "/?page=setting",
        "decorations": false,
        "titleBarStyle": "Overlay",
        "transparent": true,
        "visible": false,
        "focus": false
      },
      {
        "label": "splashscreen",
        "title": "Etcd Workbench",
        "width": 350,
        "height": 100,
        "center": true,
        "url": "splashscreen.html",
        "decorations": false,
        "resizable": false,
        "transparent": true,
        "visible": true
      }
    ],
    "macOSPrivateApi": true,
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.beifengtz.etcdworkbench",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "updater": {
      "active": true,
      "dialog": false,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEY4QUQ5NEE1MERGMjQ5NDAKUldSQVNmSU5wWlN0K0dNZDlnTlBqQzZObzBNTUxwZnFLaEhKRTBZNTFWa1piYnpvVHVmYTVIZG4K",
      "endpoints": [
        "https://vr360-beifengtz.oss-cn-beijing.aliyuncs.com/etcd-workbench/etcd-workbench-update.json"
      ],
      "windows": {
        "installMode": "passive"
      }
    }
  }
}
